PROJECT_NAME		= "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER		= "@PROJECT_VERSION@"

EXTRACT_ALL		= YES
EXTRACT_STATIC		= YES

INPUT			= "@CMAKE_SOURCE_DIR@"
RECURSIVE		= YES
FILE_PATTERNS		= *.c *.h *.sx

GENERATE_LATEX		= NO
GENERATE_HTML		= YES

HAVE_DOT		= NO

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
PREDEFINED             = __attribute__(x)=
